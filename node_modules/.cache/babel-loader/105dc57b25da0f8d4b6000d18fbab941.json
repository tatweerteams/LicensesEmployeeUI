n                    :items=\"branchWorkTimes\"\r\n                    hide-default-footer\r\n                    class=\"elevation-1 \"\r\n                  >\r\n                    <template v-slot:[`item.dayName`]=\"{ item }\">\r\n                      {{ ChangeNameDay(item) }}\r\n                    </template>\r\n                    <template v-slot:[`item.active`]=\"{ item }\">\r\n                      <v-switch\r\n                        dense\r\n                        color=\"#536DFE\"\r\n                        hide-details\r\n                        class=\"justify-center text-center mb-4  d-inline-block\"\r\n                        :input-value=\"item.isActive\"\r\n                        @click=\"activationBranchWorkTime(item)\"\r\n                      ></v-switch>\r\n                    </template>\r\n\r\n                    <template v-slot:[`item.action`]=\"{ item }\">\r\n                      <v-btn\r\n                        x-small\r\n                        fab\r\n                        color=\"#536DFE\"\r\n                        @click=\"editItem(item)\"\r\n                      >\r\n                        <v-icon color=\"#fff\">mdi-text-box-edit</v-icon>\r\n                      </v-btn>\r\n                    </template>\r\n                  </v-data-table>\r\n                </v-col>\r\n                <v-col cols=\"4\" class=\"my-4\">\r\n                  <v-card elevation=\"2\">\r\n                    <v-card-text>\r\n                      <v-row>\r\n                        <v-col cols=\"12\">\r\n                          <v-subheader class=\"font-weight-bold\">\r\n                            تعديل توقيت يوم\r\n                            <span class=\"mx-2\" v-if=\"editedItem.id\"\r\n                              >( {{ ChangeNameDay(editedItem) }} )</span\r\n                            ></v-subheader\r\n                          >\r\n\r\n                          <v-subheader class=\"font-weight-bold\"\r\n                            >ب